{
  "name": "audiofy",
  "main": "src/app.ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ios": "ns run ios",
    "android": "ns run android",
    "clean": "ns clean",
    "lint": "eslint 'src/**/*.{ts,vue}'",
    "lint:fix": "eslint 'src/**/*.{ts,vue}' --fix",
    "fmt": "prettier --write 'src/**/*.{ts,vue,json,scss}'",
    "fmt:check": "prettier --check 'src/**/*.{ts,vue,json,scss}'",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@nativescript/core": "~8.9.0",
    "@nativescript/theme": "^3.1.0",
    "nativescript-audio": "~6.2.0",
    "nativescript-vue": "~2.9.3",
    "ofetch": "^1.3.4",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@nativescript/types": "~8.9.0",
    "@nativescript/webpack": "~5.0.0",
    "@types/node": "~17.0.21",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "nativescript-vue-template-compiler": "~2.9.3",
    "prettier": "^3.6.2",
    "typescript": "~5.4.0",
    "vue": "~2.6.12"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,scss}": [
      "prettier --write"
    ]
  }
}
